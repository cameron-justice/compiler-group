/* LET EXPRESSION */
let
	/* ARRAY TYPE DECLARATION */
	type intArr = array of int

	/* VARIABLE DECLARATION */
	var size:int := 10
	var count:int := 0
	
	/* ARRAY CREATION */
	var factorialArr := intArr [size] of 0
in
	/* SEQUENCE & FOR LOOP EXPRESSION */
	for i := 0 to size-1
		/* IF-THEN-ELSE & LOGIC COMPARISON */
		do if i > 0
		then
			/* ASSIGNMENT EXPRESSION & OPERATOR PRECEDENCE & LVALUE */
			factorialArr[i] := (i + 1) * factorialArr[i-1];
		else
			/* ARITHMETIC EXPRESSION */
			factorialArr[i] := i + 1;
	
	j := 0
	/* WHILE LOOP */
	while j <> size - 1
		print(factorialArr[i]);
		j := j - 1;
end